{
  "name": "licoreriaryc-api",
  "description": "A starting template for creating Serverless and TypeScript.",
  "author": "Eduard Castellanos",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:EduardMcfly/LicoreriaRyC-api.git"
  },
  "main": ".webpack/index.js",
  "scripts": {
    "test": "jest --no-cache --coverage",
    "start": "cross-env SLS_DEBUG=* node --inspect=9000 node_modules/serverless/bin/serverless.js offline start",
    "deploy-prod": "serverless --stage=production deploy",
    "generate": "graphql-codegen"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "apollo-server-lambda": "^2.25.0",
    "cache-loader": "^4.1.0",
    "class-validator": "^0.13.1",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "dynamoose": "^2.7.3",
    "file-type": "^16.4.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "graphql-tools": "^7.0.5",
    "reflect-metadata": "^0.1.13",
    "serverless": "^2.43.1",
    "sharp": "^0.28.3",
    "twilio": "^3.66.0",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/graphql": "^14.5.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "@types/sharp": "^0.28.2",
    "@types/uuid": "^8.3.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "aws-sdk": "^2.913.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "jest": "^27.0.1",
    "jest-transform-graphql": "^2.1.0",
    "prettier": "^2.3.0",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-node-externals": "^3.0.0"
  }
}

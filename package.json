{
  "name": "licoreriaryc-api",
  "description": "A starting template for creating Serverless and TypeScript.",
  "author": "Eduard Castellanos",
  "license": "Apache-2.0",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:EduardMcfly/LicoreriaRyC-api.git"
  },
  "main": ".webpack/index.js",
  "scripts": {
    "test": "jest --no-cache --coverage",
    "start": "cross-env SLS_DEBUG=* node --inspect=9000 %USERPROFILE%\\AppData\\Roaming\\nvm\\v12.20.1\\node_modules\\serverless\\bin\\serverless offline start",
    "deploy-prod": "serverless --stage=production deploy",
    "generate": "graphql-codegen"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true
  },
  "dependencies": {
    "@types/webpack": "^5.28.0",
    "apollo-server-lambda": "^2.22.2",
    "cache-loader": "^4.1.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "dynamoose": "^2.7.3",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.4",
    "jest-transform-graphql": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.1",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.73",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.37",
